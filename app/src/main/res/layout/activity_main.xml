<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mainLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/jeans"
    tools:context=".MainActivity">


    <TextView
        android:id="@+id/textView_num_bunnies"
        android:layout_width="311dp"
        android:layout_height="50dp"
        android:layout_marginBottom="36dp"
        android:background="#172029"
        android:gravity="center"
        android:text="0 Bunnies"
        android:textColor="#FFFFFF"
        android:textSize="25dp"
        app:layout_constraintBottom_toTopOf="@+id/imageView_bunny"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.52"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/imageView_bunny"
        android:layout_width="220dp"
        android:layout_height="189dp"
        android:layout_marginTop="108dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/bunny"
        tools:srcCompat="@drawable/bunny" />

    <TextView
        android:id="@+id/textView_easterEggBasket"
        android:layout_width="250dp"
        android:layout_height="45dp"
        android:layout_marginStart="52dp"
        android:layout_marginTop="20dp"
        android:background="#3c4052"
        android:clickable="true"
        android:gravity="center"
        android:text="         Easter Egg Basket"
        android:textColor="#FFFFFF"
        android:textSize="18dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView_bunny" />

    <TextView
        android:id="@+id/textView_num_easterEggBasket"
        android:layout_width="60dp"
        android:layout_height="45dp"
        android:background="#3c4052"
        android:gravity="center"
        android:text="0"
        android:textColor="#FFFFFF"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="@+id/textView_easterEggBasket"
        app:layout_constraintStart_toEndOf="@+id/textView_easterEggBasket"
        app:layout_constraintTop_toTopOf="@+id/textView_easterEggBasket" />

    <TextView
        android:id="@+id/textView_cost_easterEggBasket"
        android:layout_width="220dp"
        android:layout_height="20dp"
        android:background="#3c4052"
        android:gravity="right"
        android:text="50 bunnies  "
        android:textColor="#FFFFFF"
        android:textSize="12dp"
        app:layout_constraintStart_toStartOf="@+id/textView_easterEggBasket"
        app:layout_constraintTop_toBottomOf="@+id/textView_easterEggBasket" />

    <ImageView
        android:id="@+id/imageView_easterEggBasket"
        android:layout_width="129dp"
        android:layout_height="121dp"
        android:layout_marginStart="4dp"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="@+id/textView_easterEggBasket"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textView_easterEggBasket"
        app:layout_constraintVertical_bias="0.413"
        app:srcCompat="@drawable/eastereggbasket"
        tools:srcCompat="@drawable/eastereggbasket" />

    <TextView
        android:id="@+id/textView_carrotFarm"
        android:layout_width="250dp"
        android:layout_height="45dp"
        android:layout_marginTop="36dp"
        android:background="#3c4052"
        android:clickable="true"
        android:gravity="center"
        android:text="Carrot Farm"
        android:textColor="#FFFFFF"
        android:textSize="18dp"
        app:layout_constraintEnd_toEndOf="@+id/textView_easterEggBasket"
        app:layout_constraintTop_toBottomOf="@+id/textView_easterEggBasket" />

    <TextView
        android:id="@+id/textView_num_carrotFarm"
        android:layout_width="60dp"
        android:layout_height="45dp"
        android:background="#3c4052"
        android:gravity="center"
        android:text="0"
        android:textColor="#FFFFFF"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="@+id/textView_carrotFarm"
        app:layout_constraintStart_toEndOf="@+id/textView_carrotFarm"
        app:layout_constraintTop_toTopOf="@+id/textView_carrotFarm"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textView_cost_carrotFarm"
        android:layout_width="220dp"
        android:layout_height="20dp"
        android:background="#3c4052"
        android:gravity="right"
        android:text="1,000 bunnies  "
        android:textColor="#FFFFFF"
        android:textSize="12dp"
        app:layout_constraintStart_toStartOf="@+id/textView_carrotFarm"
        app:layout_constraintTop_toBottomOf="@+id/textView_carrotFarm" />

    <ImageView
        android:id="@+id/imageView_carrotFarm"
        android:layout_width="161dp"
        android:layout_height="110dp"
        android:layout_marginEnd="152dp"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="@+id/textView_carrotFarm"
        app:layout_constraintEnd_toEndOf="@+id/textView_carrotFarm"
        app:layout_constraintTop_toTopOf="@+id/textView_carrotFarm"
        app:layout_constraintVertical_bias="0.371"
        app:srcCompat="@drawable/carrotfarm"
        tools:srcCompat="@drawable/carrotfarm" />

    <TextView
        android:id="@+id/textView_easterEggFactory"
        android:layout_width="250dp"
        android:layout_height="45dp"
        android:layout_marginTop="36dp"
        android:background="#3c4052"
        android:clickable="true"
        android:gravity="center"
        android:text="            Easter Egg Factory"
        android:textColor="#FFFFFF"
        android:textSize="18dp"
        app:layout_constraintEnd_toEndOf="@+id/textView_carrotFarm"
        app:layout_constraintTop_toBottomOf="@+id/textView_carrotFarm" />

    <TextView
        android:id="@+id/textView_num_easterEggFactory"
        android:layout_width="60dp"
        android:layout_height="45dp"
        android:background="#3c4052"
        android:gravity="center"
        android:text="0"
        android:textColor="#FFFFFF"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="@+id/textView_easterEggFactory"
        app:layout_constraintStart_toEndOf="@+id/textView_easterEggFactory"
        app:layout_constraintTop_toTopOf="@+id/textView_easterEggFactory" />

    <TextView
        android:id="@+id/textView_cost_easterEggFactory"
        android:layout_width="220dp"
        android:layout_height="20dp"
        android:background="#3c4052"
        android:gravity="right"
        android:text="50,000 bunnies  "
        android:textColor="#FFFFFF"
        android:textSize="12dp"
        app:layout_constraintStart_toStartOf="@+id/textView_easterEggFactory"
        app:layout_constraintTop_toBottomOf="@+id/textView_easterEggFactory" />

    <ImageView
        android:id="@+id/imageView_easterEggFactory"
        android:layout_width="121dp"
        android:layout_height="100dp"
        android:layout_marginStart="4dp"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="@+id/textView_easterEggFactory"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textView_easterEggFactory"
        app:layout_constraintVertical_bias="0.309"
        app:srcCompat="@drawable/eastereggfactory" />

    <ImageView
        android:id="@+id/imageView_bunnyHop_left"
        android:layout_width="57dp"
        android:layout_height="61dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.045"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.352"
        app:srcCompat="@drawable/left"
        tools:srcCompat="@drawable/left" />

    <ImageView
        android:id="@+id/imageView_bunnyHop_right"
        android:layout_width="57dp"
        android:layout_height="61dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.92"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.352"
        app:srcCompat="@drawable/right"
        tools:srcCompat="@drawable/right" />


</androidx.constraintlayout.widget.ConstraintLayout>